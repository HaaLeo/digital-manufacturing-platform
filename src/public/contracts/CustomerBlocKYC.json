  "source": "pragma solidity ^0.4.17;\r\n\r\nimport \"./Mortal_BlocKYC.sol\";\r\nimport \"./Storage_BlocKYC.sol\";\r\n\r\n//change contract name\r\ncontract CustomerBlocKYC is OwnedBlocKYC, MortalBlocKYC {\r\n    address private storageContractAddress;\r\n\r\n    function setStorage(address _storageContract) external onlyOwner {\r\n        storageContractAddress = _storageContract;\r\n    }\r\n    \r\n    function createCustomer(bytes32 _name, bytes32 _hashOfData) external {\r\n        StorageBlocKYC(storageContractAddress).addCustomerToCustomerData(msg.sender, _name, _hashOfData);\r\n    }\r\n\r\n    function applyToBank(address _bankAddress) external {\r\n        StorageBlocKYC(storageContractAddress).addBankToCustomer(msg.sender, _bankAddress);\r\n    }\r\n\r\n    function editCustomerData(bytes32 _hashOfData) external {\r\n        StorageBlocKYC(storageContractAddress).changeCustomerData(msg.sender, _hashOfData);\r\n    }\r\n\r\n    //Add function to get customer verification status\r\n}",
